<!DOCTYPE html>
<html>

<head>
    <title> Stanantarit krushi</title>
    <meta charset="UTF-8">
    <meta name="viewpoint" content="width=device-width, initial-scale=1">

    <link rel="stylesheet" href="tabpage.css">

    <script src="mapdata.js"></script>
    <script src="countrymap.js"></script>


</head>

<body>
    <div class="container">
       <%-include("./partials/nav")-%>
        <div class="grid-item" id="map">
            
        </div>

        <div id="selector">
            <div class="select">
                <h2>Select Crop</h2>

                <div class="select-box">
                    <div class="options-container">
                        <div class="option">
                            <input type="radio" class="radio" id="wheat" name="category" />
                            <label for="automobiles">Wheat</label>
                        </div>
                        <div class="option">
                            <input type="radio" class="radio" id="rice" name="category" />
                            <label for="automobiles">Rice</label>
                        </div>
                        <div class="option">
                            <input type="radio" class="radio" id="joawar" name="category" />
                            <label for="automobiles">jowar</label>
                        </div>
                        
                    </div>

                    <div class="selected">
                        Select a crop
                    </div>

                    <div class="search-box">
                        <input type="text" placeholder="Start Typing..." />
                    </div>
                </div>
            </div>

            <script src="tabpage.js"></script>
            

        </div>
        <div class="show_info">
            Info Above crop
        </div>

        <%-include("./partials/footer")-%>
    </div>
</body>

</html>